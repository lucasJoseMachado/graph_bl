%html{"ng-app" => "graph_bl"}
  %head
    %base{href: "/app/", target: "_blank"}
    %title="GraphBL"
    = stylesheet_link_tag    'application', media: 'all'
    = javascript_include_tag 'application'
    = csrf_meta_tags
  %body
    .row{"ng-view" => ""}
      = yield
    .row.text-center
      = image_tag("ippuj.png", class: "panel", size: "150x50")
      = image_tag("udesc.png", class: "panel", size: "250x50")
